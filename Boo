 it('should set customRange to true and selectedDateRange to calenderCustomRange', () => {
    // Arrange
    component.customRange = false;
    component.selectedDateRange = 'previousWeek'; // Initial values
    
    // Act
    component.toggleCustomRange();

    // Assert
    expect(component.customRange).toBe(true);
    expect(component.selectedDateRange).toBe('calenderCustomRange');
  });
