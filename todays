
it('should log an error when etAccountsMappingList () fails', ()=>{
const errorMessage = "Error fetching the location list!!!"; 
mockUBMService.accountsMappingList.and.returnValue (throwError(() =>errorMessage)); spyon(console, 'log'); 
component.GetAccountsMappingList();
expect (console.log).toHaveBeenCalledwith(errorMessage);
});
