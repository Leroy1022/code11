
it('should log an error when GetAccountsMappingList () fails', ()=>{
const errorMessage = "Error fetching the location list!!!"; 
mockUBMService.GetAccountsMappingList.and.returnValue (throwError(() =>errorMessage)); spyon(console, 'log'); 
component.accountsMappingList  ();
expect (console.log).toHaveBeenCalledwith(errorMessage);
});
