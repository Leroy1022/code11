 it('should not change actionsClicked property when toggled', () => {
    // Arrange
    const initialActionsClicked = component.actionsClicked;

    // Act
    component.toggleActionsDropdown();

    // Assert
    expect(component.actionsClicked).toBe(initialActionsClicked);
  });

