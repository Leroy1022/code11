  it('should count selected items correctly', () => {
    // Arrange
    component.result = [
      { IsRowChecked: true },
      { IsRowChecked: false },
      { IsRowChecked: true },
    ];

    // Act
    component.countSelected();

    // Assert
    expect(component.selectedCount).toBe(2);
  });

